        }

        .tabs {
            border: 1px solid #000000;
            text-align: left;
            padding: 6px;
        }

        .samasz {
            text-align: right;
            color: green;
        }

        .samasz2 {
            color: green;
        }

        .title {
            text-align: center;
            padding: 0px;
            margin: 0px;
        }
    </style>
</head>
<body>
    <p class="samasz">SaMASZ®</p>
    <hr class="solid">
    <h1 class="title"><strong>Jednopunktowa lekcja</strong></h1>
    <hr class="solid">
    <br>
    <table>
        <tr class="tabs">
            <td class="tabsl">Obszar: Produkcja</td>
            <td class="tabs">Sporządził: Marcin Lange</td>
        </tr>
        <tr class="tabs">
            <td class="tabsl"><strong>Temat:  @Html.DisplayFor(model => model.Temat)</strong></td>
            <td class="tabs">Sprawdził: Wojciech Grelecki</td>
        </tr>
        <tr class="tabs">
            <td class="tabsl"></td>
